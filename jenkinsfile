pipeline
{
  agent any
    triggers
            {
              cron("*****")
            }
    stages
          {
              stage("compiling")
              {
                steps
                {
                  echo "compiling"
                  sh "javac hello.java"
                }
              }
                stage("running")
                {
                  steps
                    {
                      echo "executing"
                      sh "java hello"
                    }
                }
          }
  }
