pipeline
{
  agent any
    triggers
            {
              cron(" * * * * * ")
            }
    stages
          {
              stage("compiling")
              {
                steps
                {
                  echo "compiling"
                  sh "javac Hello_RIT.java"
                }
              }
                stage("running")
                {
                  steps
                    {
                      echo "executing"
                      sh "java Hello_RIT"
                    }
                }
          }
  }
